            // Console.ReadKey();

            int number1 = 0;
            int number2 = 0;
            int total = 0;

            number1 = 50;
            number2 = 100;
            total = number1 * number2;

            Console.WriteLine("The answer is total : " + total);
            Console.WriteLine("{0} {1}", "The product of the number : ",  total);
            Console.ReadKey();
