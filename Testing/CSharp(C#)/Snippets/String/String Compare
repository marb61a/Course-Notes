using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace StringEx1
{
    class StringEx
    {
        static void Main(string[] args)
        {
            string str1 = "Martin";
            string str2 = "Martin";
            string str3 = new string(new char[]
            {
                'M', 'a', 'r', 't', 'i', 'n'
            });

            Console.WriteLine(str1 == str2);
            Console.WriteLine(str1 == str3);
            Console.WriteLine(str2 == str3);

            string str4 = "Hell";
            string str5 = "o W";
            string str6 = "orld";
            Console.WriteLine(str4 + str5 + str6);

            Console.ReadKey();
        }
    }
}
