using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace LoopsEx
{
    class Program
    {
        public static void forEachExample()
        {
            int[] fibArray = new int[] { 0, 1, 1, 2, 3, 5, 8, 13 };

            // The same as an enhanced for loop in Java
            foreach(int numberElement in fibArray)
            {
                Console.WriteLine(numberElement);
            }

            Console.WriteLine("\n");

            // Will do the same as the above loop
            for(int i = 0; i < fibArray.Length; i++)
            {
                Console.WriteLine(fibArray[i]);
            }
        }

        static void Main(string[] args)
        {
            forEachExample();

            Console.ReadKey();
        }
    }
}
