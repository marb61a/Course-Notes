using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace Exercise4Q4
{
    class Program
    {
        static void Main(string[] args)
        {
            String one = "Hell";
            String two = "Hel";
            String three = "l";
            String four = "o";
            String five;
            String six;

            two = two + three;
            five = two + four;
            six = one + four;

            Console.WriteLine(one == two);          // True
            Console.WriteLine(one == five);         // False
            Console.WriteLine(five == six);         // True

            Console.ReadKey();
        }
    }
}
