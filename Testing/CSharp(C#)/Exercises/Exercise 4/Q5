using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace Exercise4Q5
{
    class Exercise4Q5
    {
        static void Main(string[] args)
        {
            String firstString;
            String secondString;

            Console.Write("Please enter a value ");
            firstString = Convert.ToString(Console.ReadLine());

            Console.Write("Please enter a second value ");
            secondString = Convert.ToString(Console.ReadLine());

            Console.WriteLine(firstString == secondString);
            Console.WriteLine(firstString.Equals(secondString));
            Console.WriteLine(String.Compare(firstString, secondString, true) == 0 );

            Console.ReadKey();
        }
    }
}
