using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace Exercise4Q3
{
    class Exercise4Q3
    {
        static void Main(string[] args)
        {
            String name1 = "Sandra";
            String name2 = "Sandra";
            String name3 = new string(new char[] {
                'S', 'a', 'n', 'd', 'r', 'a'
            });

            Console.WriteLine(name1 == name2);
            Console.WriteLine(name1 == name3);
            Console.WriteLine(name2 == name3);

            Console.ReadKey();
        }
    }
}
