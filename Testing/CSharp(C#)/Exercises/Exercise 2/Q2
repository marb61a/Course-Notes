using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace Exercise2Q2
{
    class Exercise2Q2
    {
        static void Main(string[] args)
        {
            int number1 = 5;
            int number2 = 16;
            int number3;

            Console.WriteLine("First number before swapping is {0}", number1);
            Console.WriteLine("Second number before swapping is {0}", number2);

            number3 = number2;
            number2 = number1;
            number1 = number3;

            Console.WriteLine("First number after swapping is {0}", number1);
            Console.WriteLine("Second number after swapping is {0}", number2);

            Console.ReadKey();
        }
    }
}
