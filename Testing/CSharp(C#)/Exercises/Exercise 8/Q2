using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace Exercise8
{
    class Rectangle : Shape
    {
        public Rectangle(double length, double width)
        {
            Length = Length;
            Width = width;
        }

        public override double getArea()
        {
            return Width * Length;
        }
    }
}
