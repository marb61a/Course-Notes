using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace Exercise9
{
    public class Vehicle
    {
        private string make;
        private string model;
        public double engineSize;

        public string Make
        {
            get
            {
                return make;
            }
            set
            {
                make = value;
            }
        }
        public string Model
        {
            get
            {
                return model;
            }
            set
            {
                model = value;
            }
        }
        public double EngineSize
        {
            get
            {
                return engineSize;
            }
            set
            {
                engineSize = value;
            }
        }

        public Vehicle()
        {

        }
        public Vehicle(string make, string model, double engineSize)
        {
            this.make = make;
            this.model = model;
            this.engineSize = engineSize;
        }


    }
}
