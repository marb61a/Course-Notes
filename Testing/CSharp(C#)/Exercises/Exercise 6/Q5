using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace Exercise6Q5
{
    class Exercise6Q5
    {
        static void Main(string[] args)
        {
            int a = 0;

            string[] enteredNames = new string[5];
            while(a < 5)
            {
                Console.Write("Enter A Name ");

                enteredNames[a] = Console.ReadLine();

                a++;
            }

            foreach (var el in enteredNames)
            {
                Console.WriteLine("The names are {0} ", el);
            }

            Console.ReadKey();
        }
    }
}
