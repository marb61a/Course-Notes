using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace Exercise6Q7
{
    class Exercise6Q7
    {
        static void Main(string[] args)
        {
            int[] test = new int[] { 0, 1, 1, 2, 3, 5, 8, 13 };

            foreach(int el in test)
            {
                Console.WriteLine("The numbers were {0}", el);
            }

            Console.ReadKey();
        }
    }
}
