CREATE TABLE gogreen(
	ServiceNo float NULL,
	CusNam nvarchar(255) NULL,
	FillHours float NULL,
	Discrip nvarchar(255) NULL,
	Price float NULL,
	vat float NULL,
	total float NULL
);

