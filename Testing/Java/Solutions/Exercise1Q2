Escaping Characters

package org.java.exercisesoneqtwo;

class StringPrint {
	public static void main(String[] args) {
		System.out.print(
		"This sentence begins at the first position on the line"+"\n"
		+"\t"+"whereas this line starts at the first tab position"+"\n"+
		"\t"+"\t"+"and this at the second tab position"+"\n"
		+"Now "+"I'm "+"going to print "+"\"a phrase in double quotes\""+"\n"
		+"That's"+"\t"+"the"+"\t"+"end"
		);
	}
}
