Converting a currency amount into component notes and coins

package org.java.exercisetwoqthree;

import java.util.*;

public class MoneyCount {
	public static void main(String[] args) {
		Scanner myScan = new Scanner(System.in);
		
		try {
			System.out.println("Please enter a sum of money");
			double numMoney = myScan.nextDouble();
			
			int twentyNotes = (int)numMoney / 20;
			int tenNotes = (int);
			int fiveNotes = (int);
			int oneCoins = (int);
			
			System.out.printf("You have Twenties %d" = "You have tens %d", twentyNotes, tenNotes);
			
		} catch(ArithmeticException | InputMismatchException ex){ 
			System.out.println("Exception: " + ex.toString());
		}
	}
}
