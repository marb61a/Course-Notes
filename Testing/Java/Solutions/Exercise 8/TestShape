public class TestShape {
    public static void main(String[] args){
        Shape shape1 = new Shape();
        System.out.println("The shape color is " + shape1.getColor() + " is filled " + shape1.isFilled());

        Shape shape2 = new Circle(2.0);
        System.out.println("The shape color is " + shape2.getColor() + " is filled " + shape2.isFilled() + ", which is a subclass of " + super.toString());
    }
}
