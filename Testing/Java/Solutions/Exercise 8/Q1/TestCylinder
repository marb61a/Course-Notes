public class TestCylinder {
    public static void main(String[] args){
        Cylinder newCyl = new Cylinder();
        
        System.out.println("Cylinder : "
        + "radius = " + newCyl.getRadius() 
        + " height = " + newCyl.getHeight()
        + " base area = " + newCyl.getArea()
        + " volume = " + newCyl.getVolume()
        );
        
        Cylinder newCy2 = new Cylinder(10.0);
        
        System.out.println("Cylinder : "
        + "radius = " + newCy2.getRadius() 
        + " height = " + newCy2.getHeight()
        + " base area = " + newCy2.getArea()
        + " volume = " + newCy2.getVolume()
        );
        
        Cylinder newCy3 = new Cylinder(2.0, 10.0);
        
        System.out.println("Cylinder : "
        + "radius = " + newCy3.getRadius() 
        + " height = " + newCy3.getHeight()
        + " base area = " + newCy3.getArea()
        + " volume = " + newCy3.getVolume()
        );
    }
}
