import java.util.*;

public class MySubClassExample extends MySuperClassExample{
	int b = 3;
	
	public void myMethod(){
		System.out.print("y" + b);
	}
	
	public static void main(String[] args){
		MySubClassExample mySubEx = new MySubClassExample();
	}
}

class MySuperClassExample{
	int a = 4;
	
	public MySuperClassExample(){
		myMethod();
	}
	
	void myMethod(){
		a++;
		System.out.print("y" + a);
	}
}
