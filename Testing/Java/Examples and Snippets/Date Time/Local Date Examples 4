import java.time.*; 
import java.time.format.*;
import java.time.temporal.*;

public class LocalDateTimeExamplesFour{
    public static void main(String[] args) {
        // The toString() method examples
        // Get the LocalDateTime instance 
        LocalDateTime dt = LocalDateTime.now(); 
  
        // Get the String representation of this LocalDateTime using the toString() method 
        System.out.println(dt.toString());

        // create an LocalDateTime object 
        LocalDateTime lt = LocalDateTime.parse("2019-06-28T19:15:12"); 
        System.out.println(lt.toString()); 
        
    }
}
