An example of the 3 main loops

package org.java.varloopsexample;

public class VariousLoops {
	public static void main(String[] args){
		System.out.println("An example of a nested for loop");
		for(int i = 0; i < 6; i++){			
			char upperCaseA = 'A';
			
			for(int j = 0; j < 26; j++){
				System.out.print(upperCaseA);
				upperCaseA++;
			}
			
			System.out.println();
		}
		System.out.println();
		
		System.out.println("An example of a nested while loop");
			int x = 0;
			while(x < 10){
				char upperCaseA = 'A';
				int y = 0;
				
				while(y < 26){
					System.out.print(upperCaseA);
					upperCaseA++;
					y++;
				}
				
				System.out.println();
				x++;
			}
		System.out.println();
		
		System.out.println("An example of a nested do-while loop");
			int k = 0;
			do{
				char upperCaseA = 'A';
				int l = 0;
				do{
					System.out.print(upperCaseA);
					upperCaseA++;
					l++;
				} while (l < 26);
				System.out.println();
				k++;
			} while (k < 6);
		
		System.out.println();
	}
}
